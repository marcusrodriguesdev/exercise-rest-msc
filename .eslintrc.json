{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "prefer-promise-reject-errors": "off"
    }
}
